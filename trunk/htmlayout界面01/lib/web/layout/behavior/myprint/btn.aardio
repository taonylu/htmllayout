//打印按钮
namespace web.layout.behavior.myprint.btn

onButtonClick = {
	//打印预览
    print = function(ltTarget, ltEle, x, y, mouseParams){
    	var print_content = ltEle.root().queryElement("#"++ltEle.getAttribute("data-id")); //指定要打印的区域
		var html = print_content.innerHTML;
        web.layout.behavior.myprint.page.print(html);
    }
}

